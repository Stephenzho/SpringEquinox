<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="petStore" class="cn.shuyiio.springequinox.service.v2.PetPostService2">
        <property name="itemDao" ref="itemDao"/>
        <property name="accountDao" ref="accountDao"/>
        <property name="name" value="zhoushuyi"/>
        <property name="version" value="1"/>
    </bean>


    <bean id="accountDao" class="cn.shuyiio.springequinox.dao.AccountDao"/>

    <bean id="itemDao" class="cn.shuyiio.springequinox.dao.ItemDao"/>


</beans>